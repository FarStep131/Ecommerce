<h1>Admins::Products#index</h1>
<p>Find me in app/views/admins/products/index.html.erb</p>

<% @products.each do |product| %>
  <div>
    <div><%= image_tag product.image.to_s, :size => '200x200' %></div>
    <div><%= product.name %></div>
    <div><%= product.description %></div>
    <div><%= product.genre %></div>
    <div><%= product.price %></div>
    <div><%= product.sales_status %></div>
    <%= link_to "詳細",  admins_product_path(product) %>
  </div>
<% end %>
